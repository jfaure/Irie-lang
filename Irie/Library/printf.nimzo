type Arrow retTy = Stack (->) retTy

printf : String -> Arrow String
printf (fmt : String)
= case fmt of
    "%d" ++ r => \(d:Int   ) => show d ++ printf r
    "%f" ++ r => \(f:Float ) => show f ++ printf r
    "%s" ++ r => \(s:String) => s      ++ printf r
    "%%" ++ r => '%' , printf r
    c , r     =>  c  , printf r
