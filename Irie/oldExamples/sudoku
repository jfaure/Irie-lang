type Grid = [9 9] * (Integer 16)

eg = "9...7....|2...9..53|.6..124..|84...1.9.|5.....8..|.31..4...|..37..68.|.9..5.741|47......."

strToGrid : String -> Maybe Grid
 = \s -> go s
   where go s =
     let line & r = splitAt 9 s
     in case r of
       '|' , lines -> (line ;) <$> go lines
       _ -> Nothing
