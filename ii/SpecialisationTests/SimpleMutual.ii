-- Mutual specialisation

go2 z i next s = case next s of
  @Done      => go1 z i
  @Skip s    => go2 z i next s
  @Yield x s => go2 (add z x) i next s

nextRet = \case
  @True  => Yield (mul 0 0 {-i i-}) @False
  @False => Done

